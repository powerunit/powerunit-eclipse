<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">
  <poweredBy>
    <logo name="PowerUnit" href="http://www.powerunit.ch/"
          img="http://www.powerunit.ch/powerunit/logo.png"/>
    <logo name="Travis-ci" href="https://travis-ci.org/powerunit/powerunit-eclipse"
          img="https://travis-ci.org/powerunit/powerunit-eclipse.svg?branch=master"/>
    <logo href="http://marketplace.eclipse.org/marketplace-client-intro?mpc_install=2412591" 
    	  name="Drag to your running Eclipse workspace to install PowerUnit Eclipse Integration"
    	  img="https://marketplace.eclipse.org/sites/all/themes/solstice/_themes/solstice_marketplace/public/images/btn-install.png"/>
  </poweredBy>
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.1</version>
  </skin>
  <custom>
    <fluidoSkin>
      <googleSearch>
      </googleSearch>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <gitHub>
        <projectId>powerunit/powerunit-eclipse</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>black</ribbonColor>
      </gitHub>
    </fluidoSkin>
  </custom>
  <body>
    <menu ref="reports" />
    <menu name="General documentations">
      <item name="Manual" href="manual.html" />
    </menu>
  </body>
</project>